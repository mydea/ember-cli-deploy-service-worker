{
  "name": "ember-cli-deploy-service-worker",
  "version": "0.1.3",
  "description": "Generate a sw.js file to cache assets at build/deploy time.",
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin"
  ],
  "repository": "https://github.com/mydea/ember-cli-deploy-service-worker",
  "license": "MIT",
  "author": "Francesco Novy <francesconovy@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:node": "mocha node-tests/index-test.js"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.6",
    "ember-cli-deploy-plugin": "^0.2.9",
    "glob": "^7.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "~7.16.3",
    "@ember/optional-features": "~2.0.0",
    "@ember/test-helpers": "~2.6.0",
    "@embroider/test-setup": "~0.47.2",
    "@glimmer/component": "~1.0.4",
    "@glimmer/tracking": "~1.0.4",
    "babel-eslint": "~10.1.0",
    "broccoli-asset-rev": "~3.0.0",
    "chai": "~4.3.4",
    "chai-as-promised": "~7.1.1",
    "ember-auto-import": "~2.2.4",
    "ember-cli": "~3.28.4",
    "ember-cli-dependency-checker": "~3.2.0",
    "ember-cli-htmlbars": "~6.0.0",
    "ember-cli-inject-live-reload": "~2.1.0",
    "ember-cli-sri": "~2.1.1",
    "ember-cli-terser": "~4.0.2",
    "ember-disable-prototype-extensions": "~1.1.3",
    "ember-export-application-global": "~2.0.1",
    "ember-load-initializers": "~2.1.2",
    "ember-qunit": "~5.1.4",
    "ember-resolver": "~8.0.2",
    "ember-source": "~3.28.0",
    "ember-source-channel-url": "~3.0.0",
    "ember-try": "~2.0.0",
    "eslint": "~8.3.0",
    "fabscale-eslint-config": "~1.2.0",
    "fs-extra": "~10.0.0",
    "loader.js": "~4.7.0",
    "mocha": "~9.1.3",
    "npm-run-all": "~4.1.5",
    "prettier": "~2.5.0",
    "qunit": "~2.17.2",
    "qunit-dom": "~2.0.0",
    "release-it": "~14.11.5",
    "release-it-lerna-changelog": "~4.0.1",
    "webpack": "~5.64.4"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  },
  "volta": {
    "node": "16.13.0",
    "yarn": "1.22.17"
  }
}
